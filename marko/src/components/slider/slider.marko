import moment from 'moment'

div.slider-date key='slider'
div.dates
  $ const formatDate = (date) => moment(date).format('MMM DD, YYYY HH:mm')
  div
    span -- Begin:
    span -- ${formatDate(state.currentStart)}
  div
    span -- End:
    span -- ${formatDate(state.currentEnd)}
  div
button.uk-button.uk-button-primary.update-data style='float:right;' on-click('updateInterval') -- Update
